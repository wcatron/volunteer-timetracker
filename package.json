{
  "name": "volunteer-timetracker",
  "version": "1.0.0",
  "description": "Time tracking software for volunteers.",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run build-server && npm run build-production && npm run build-semantic-ui",
    "build-semantic-ui": "cd semantic; gulp build",
    "test": "mocha",
    "start": "nf build-server/server.js",
    "build-server": "tsc --project tsconfig.server.json",
    "build-server+watch": "tsc --project tsconfig.server.json --watch",
    "build-production": "webpack --config webpack.production.config.js",
    "build-production+watch": "webpack --config webpack.production.config.js --watch",
    "tsc": "node_modules/.bin/tsc",
    "start-dev": "export NODE_ENV=development && node build-server/server.js"
  },
  "author": "Weston Catron",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/react-router": "^3.0.15",
    "@types/react-router-redux": "^4.0.51",
    "@types/redux-devtools-dock-monitor": "^1.1.33",
    "@types/redux-devtools-log-monitor": "^1.0.34",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "csv-parse": "^2.5.0",
    "csv-stringify": "^4.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.4.2",
    "react-datetime-picker": "^1.5.0",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
